
<html metal:use-macro="context/@@groupserver_site_home_layout/page">
  <head>
    <title metal:fill-slot="title">Group Stats:
      <span tal:replace="view/siteInfo/name"/>
    </title>
    <style metal:fill-slot="style" type="text/css">
      #gs-group-stats-table {
        position: relative; /* New position context */
      }

      #gs-group-stats-table .e {
        background: #EDF1E4;
      }
      #gs-group-stats-table thead {
        text-align: right;
        float: left;
        width: 200px;
      }
      #gs-group-stats-table thead th {
        text-align: right;
      }
      #gs-group-stats-table tbody {
        float: right;
        position: relative;
        overflow: auto;
        width: 722px;
      }
      #gs-group-stats-table td {
        text-align: right;
      }
      #gs-group-stats-table .ratio {
        font-style: oblique;
      }
    </style>
  </head>
  <body>
    <tal:block metal:fill-slot="contextnavigation">&#160;</tal:block>
    <div id="gs-stats-group-historical" metal:fill-slot="body">

  <p tal:define="groupId string:mpls; siteId string:main"
    tal:content="structure provider:groupserver.HistoricalGroupStats">
  </p>           
    </div>
  </body>
</html>
