
<html metal:use-macro="context/@@groupserver_site_home_layout/page">
  <head>
    <title metal:fill-slot="title">Group Stats:
      <span tal:replace="view/siteInfo/name"/>
    </title>
  </head>
  <body>
    <tal:block metal:fill-slot="contextnavigation">&#160;</tal:block>
    <div id="gs-stats-group" metal:fill-slot="body"
 tal:define="munge_date python:modules['Products.XWFCore.XWFUtils'].munge_date">

    <table>
    <tr><td>authors</td><td><span tal:replace="python:view.authors()"/></td></tr>   
    <tr><td>posts</td><td><span tal:replace="python:view.posts()"/></td></tr>
    <tr><td>new topics</td><td><span tal:replace="python:view.new_topics()"/></td></tr>
    <tr><td>active topics</td><td><span tal:replace="python:view.active_topics()"/></td></tr>
    <tr><td>join delta</td><td><span tal:replace="python:view.join_delta()"/></td></tr>
    <tr><td>average members</td><td><span tal:replace="python:view.member_average()"/></td></tr>
    <tr><td>current member count</td><td><span tal:replace="python:view.current_member_count()"/></td></tr>
    </table>
    </div><!--gs-stats-group->
  </body>
</html>
